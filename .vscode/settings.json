{
  "i18n-ally.localesPaths": [
    "locales"
  ],
  "files.watcherExclude": {
    "**/node_modules/**": true,
    "**/.git/**": true,
    "**/.next/**": true,
    "**/build/**": true,
    "**/dist/**": true,
    "**/.vercel/**": true,
    "**/coverage/**": true,
    "**/.nyc_output/**": true,
    "**/tmp/**": true,
    "**/temp/**": true,
    "**/.cache/**": true,
    "**/logs/**": true,
    "**/*.log": true,
    "**/package-lock.json": true,
    "**/yarn.lock": true,
    "**/.turbo/**": true
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/.next": true,
    "**/coverage": true,
    "**/.git": true,
    "**/dist": true,
    "**/build": true,
    "**/.cache": true,
    "**/logs": true,
    "**/.turbo": true
  },
  "files.exclude": {
    "**/node_modules": true,
    "**/.git": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true
  },
  "typescript.preferences.includePackageJsonAutoImports": "off",
  "typescript.suggest.autoImports": false,
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "files.autoSave": "onFocusChange",
  "extensions.ignoreRecommendations": true
}