{
  "ci": {
    "collect": {
      "url": [
        "http://localhost:3000/",
        "http://localhost:3000/sign-in",
        "http://localhost:3000/organizations",
        "http://localhost:3000/people",
        "http://localhost:3000/dashboard"
      ],
      "numberOfRuns": 3,
      "startServerCommand": "npm run start",
      "startServerReadyPattern": "ready on",
      "settings": {
        "preset": "desktop",
        "throttlingMethod": "devtools",
        "onlyCategories": ["performance", "accessibility", "best-practices", "seo"],
        "skipAudits": ["uses-http2", "redirects-http", "uses-long-cache-ttl"],
        "extraHeaders": "{\"Cookie\": \"next-auth.session-token=dummy-session\"}"
      }
    },
    "assert": {
      "preset": "lighthouse:no-pwa",
      "assertions": {
        "categories:performance": ["error", {"minScore": 0.7}],
        "categories:accessibility": ["error", {"minScore": 0.9}],
        "categories:best-practices": ["error", {"minScore": 0.8}],
        "categories:seo": ["error", {"minScore": 0.8}],
        "first-contentful-paint": ["error", {"maxNumericValue": 2000}],
        "largest-contentful-paint": ["error", {"maxNumericValue": 3000}],
        "cumulative-layout-shift": ["error", {"maxNumericValue": 0.1}],
        "total-blocking-time": ["error", {"maxNumericValue": 500}],
        "interactive": ["error", {"maxNumericValue": 5000}],
        "max-potential-fid": ["error", {"maxNumericValue": 200}],
        "server-response-time": ["error", {"maxNumericValue": 600}],
        "uses-responsive-images": "warn",
        "uses-optimized-images": "warn",
        "uses-text-compression": "warn",
        "uses-rel-preconnect": "warn",
        "font-display": "warn"
      }
    },
    "upload": {
      "target": "temporary-public-storage",
      "reportFilenamePattern": "pantrycrm-%%PATHNAME%%-%%DATETIME%%-report.%%EXTENSION%%"
    },
    "server": {
      "port": 9001,
      "storage": {
        "storageMethod": "sql",
        "sqlDatabasePath": ".lighthouseci/db.sql"
      }
    }
  }
}