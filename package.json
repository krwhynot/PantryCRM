{
  "name": "nextcrm-app",
  "version": "0.0.3-beta",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "cross-env prisma generate && cross-env prisma db push && cross-env prisma db seed && cross-env NODE_OPTIONS=\"--max-old-space-size=4096\" next build",
    "build:safe": "cross-env prisma generate && cross-env prisma db push && cross-env prisma db seed && cross-env NODE_OPTIONS=\"--max-old-space-size=4096 --no-warnings\" NEXT_TELEMETRY_DISABLED=1 next build",
    "build:azure": "prisma generate && NODE_OPTIONS=\"--max-old-space-size=2048\" NEXT_TELEMETRY_DISABLED=1 CI=true DATABASE_URL=\"postgresql://dummy:dummy@localhost:5432/dummy\" GOOGLE_ID=\"dummy\" GOOGLE_SECRET=\"dummy\" GITHUB_ID=\"dummy\" GITHUB_SECRET=\"dummy\" JWT_SECRET=\"dummy-secret-for-build\" next build",
    "start": "next start",
    "start:azure": "next start -p $PORT",
    "lint": "next lint",
    "test": "jest --watchAll",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:client": "source-map-explorer .next/static/chunks/*.js",
    "analyze:report": "npm run build && npm run analyze:client",
    "analyze:bundle": "node ./scripts/analyze-bundle.js",
    "analyze:simple": "node ./scripts/simple-bundle-analyzer.js",
    "optimize": "npm dedupe && npm prune",
    "clean": "if exist .next rmdir /s /q .next",
    "prebuild": "rimraf .next",
    "build:prod": "npm run prebuild && npm run build"
  },
  "prisma": {
    "seed": "node ./prisma/seed.js"
  },
